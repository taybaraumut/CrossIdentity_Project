@using CrossIdentityProject.UI.Models.CityModels
@model List<CityModel>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Panel_Layout.cshtml";
}
<div class="container mt-5">
    <div class="row">
        <div class="col-12">
            <div class="text-center font-weight-bold">
                <h1>Kullanıcı Panel Sistemi</h1>
            </div>
        </div>
    </div>
</div>
<table class="table table-bordered">
    <thead>
        <tr>
            <th scope="col">Ülke Id</th>
            <th scope="col">Ülke Adı</th>
        </tr>
    </thead>
    <tbody>
        @for (int i = 0; i < Model.Count; i++)
        {
            <tr>
                <td>@Model[i].CityID</td>
                <td>@Model[i].CityName</td>
            </tr>
        }            
    </tbody>
</table>

